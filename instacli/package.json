{
  "name": "instagram-agent-cli",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "railway:build:central": "pnpm --filter @instacli/ig-core build && pnpm --filter @instacli/central-api build",
    "railway:start:central": "pnpm --filter @instacli/central-api start",
    "railway:build:landing": "pnpm --filter @instacli/landing build",
    "railway:start:landing": "pnpm --filter @instacli/landing start"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "turbo": "^2.5.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.5"
  }
}
